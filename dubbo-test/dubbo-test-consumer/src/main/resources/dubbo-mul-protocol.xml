<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
       http://dubbo.apache.org/schema/dubbo
       http://dubbo.apache.org/schema/dubbo/dubbo.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <dubbo:application name="dubbo-mul-protocol"/>

    <dubbo:registry address="nacos://139.196.154.217:8848" id="nacos"/>

    <dubbo:protocol name="rmi" port="20881"/>

    <dubbo:protocol name="dubbo" port="20880"/>

    <dubbo:reference id="demoService" interface="com.alibaba.service.DemoService" protocol="rmi" registry="nacos"/>

</beans>